{
  "name": "cypresscucumber",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "test": "cypress run",
    "test:chrome": "cypress run --browser chrome --no-exit",
    "test:allure": "cypress run --config video=false --env allure=true",
    "allure:generate": "allure generate ./test-report/allure-results -o ./test-report/allure-report --clean",
    "allure:open": "allure open ./test-report/allure-report",
    "code:check": "yarn lint && yarn code:prettier",
    "code:format": "yarn lint --fix --quiet && yarn code:prettier --write",
    "code:gherkin": "yarn gherkin-lint -c .gherkin-lintrc.json cypress/integration",
    "lint": "./node_modules/.bin/eslint .",
    "code:prettier": "./node_modules/.bin/prettier --check \"**/*.js*\""
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/steps"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.1.0",
    "gherkin-lint": "^4.2.1",
    "husky": "^5.1.3",
    "node-oracledb": "^1.0.2",
    "oracledb": "^5.1.0",
    "pg": "^8.5.1",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.3.5",
    "allure-commandline": "^2.13.8",
    "cypress": "^6.5.0",
    "cypress-cucumber-preprocessor": "^4.0.1",
    "eslint": "^7.21.0"
  },
  "type": "module"
}